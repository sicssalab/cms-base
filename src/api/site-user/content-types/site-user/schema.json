{
  "kind": "collectionType",
  "collectionName": "site_users",
  "info": {
    "singularName": "site-user",
    "pluralName": "site-users",
    "displayName": "Site_User",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string"
    },
    "lada_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::lada.lada",
      "inversedBy": "site_users"
    },
    "celphone": {
      "type": "biginteger",
      "required": true,
      "unique": true,
      "max": "9999999999"
    },
    "birthday": {
      "type": "date"
    },
    "email": {
      "type": "email",
      "required": true,
      "maxLength": 250,
      "minLength": 5,
      "unique": true
    },
    "gender_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::gender-user.gender-user",
      "inversedBy": "site_users"
    },
    "password_view": {
      "type": "string",
      "required": true,
      "minLength": 6,
      "private": true
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "page_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::page-user.page-user",
      "inversedBy": "site_users"
    }
  }
}
